{
  "manifest_version": 2,

  "name": "01 - Request Redirect Demo",
  "description": "Forward baidu search to google.",
  "version": "1.0.0",

  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },

  "background": {
    "scripts": ["event.js"],
    "persistent": true
  },

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.baidu.com/*"
  ]
}
